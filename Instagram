#include <stdio.h>

int main(void)
{
     
         int i,T;
         scanf("%d",&T);
         for(i=1;i<=T;i++)
         {
             int x,y;
             scanf("%d%d",&x,&y);
             if(x>10*y)
             {
             printf("YES\n");
             }
             else
             {
        printf("NO\n");
                 
                
             }
             
         }
         return 0;
         
     }
	
